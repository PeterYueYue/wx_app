.pages{
  height: 100vh;
  background: url("http://sbag-small.oss-cn-huhehaote.aliyuncs.com/upload/img/web/yy/vouchers/bg2.png") no-repeat;
  background-size: 100% 100%;
} 
.head{
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.head .icon{
  width: 120rpx;
  height: 120rpx;
  margin-top: 58rpx;

}
.head .dhk{
  width: 520rpx;
  height: 156rpx;
  
  background: url("http://sbag-small.oss-cn-huhehaote.aliyuncs.com/upload/img/web/yy/vouchers/dhk.png") no-repeat;
  background-size: 100% 100%;
  margin-top: 10rpx;
  
}
.dhk .text{
  line-height: 48rpx;
  font-size: 30rpx;
  color: #3498FF;
  margin-top: 40rpx;
  padding: 0 30rpx;
  
}
.carbag{
  width: 735rpx;
  height: 693rpx;
  background: url("http://sbag-small.oss-cn-huhehaote.aliyuncs.com/upload/img/web/yy/vouchers/bg3.png") no-repeat;
  background-size: 100% 100%;
  margin-top: 36rpx;
  margin-left: auto;
  margin-right: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  
}
.carbag .pic{
  width: 120rpx;
  height: 120rpx;
  margin-top: 52rpx;
}
.title1{
  color: #FF9626;
  font-size: 36rpx;
  margin-top: 10rpx;

}
.title2{
  color: #666666;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.sysm{
  margin-top: 20rpx;
}
.item{
  width: 500rpx;
  display: flex;
  color: #666666;
  font-size: 22rpx;
  flex-direction: row;
  margin-bottom: 10rpx;
}
.item .key{
  width: 150rpx;
  text-align: right;
  font-weight: bold;
  
}
.item .val{
  flex: 1;

  
}
.btn{
  width:520rpx;
  height:44px;
  background:linear-gradient(179deg,rgba(255,240,205,1),rgba(255,229,164,1));
  border-radius:43rpx;
  position: absolute;
  left: 107rpx;
  bottom: 96rpx;
  font-size: 18px;
  line-height: 44px;
  color: #B93A24;
  text-align: center;
}
/* 提示弹窗 */
.maskBox{
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,0.6);
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  
}
.maskBox .content{
  width:610rpx;
  height:540rpx;
  background:rgba(255,255,255,1);
  border-radius:20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.maskBox .title{
  font-size: 38rpx;
  color: #006EE7;
  margin-top: 30rpx;
}
.maskBox .img{
  width: 240rpx;
  height: 200rpx;
  margin-top: 12rpx;
}
.maskBox .remark{
  font-size: 28rpx;
  color: #666666;
  width: 430rpx;
  margin-top: 20rpx;
  text-align: center;
  
}
.maskBox .jumbtn{
  width:440rpx;
  height:44px;
  background:rgba(58,165,255,1);
  border-radius:44rpx;
  font-size: 18px;
  line-height: 44px;
  color: #FFFFFF;
  text-align: center;
  position: absolute;
  bottom:   38rpx;
  left: 85rpx;

}
.sucColor{

  color: #FF6915 !important;
}