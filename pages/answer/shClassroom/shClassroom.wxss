.page{
  width: 100%;
  height: 100vh;
  position: relative;
  background: url("https://sbag-small.oss-cn-huhehaote.aliyuncs.com/upload/img/web/yy/answer/bg.png") no-repeat;
  background-size: 100% 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content{
  width: 690rpx;
  height: 1110rpx;
  background: url("https://sbag-small.oss-cn-huhehaote.aliyuncs.com/upload/img/web/yy/answer/bg1.png") no-repeat;
  background-size: 100% 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content .btn1{
  width: 280rpx;
  height: 80rpx;
  margin-top: 640rpx;
}
.content .btn2{
  width: 280rpx;
  height: 80rpx;
  margin-top: 24rpx;
}
.content .btn3{
  width: 500rpx;
  height: 116rpx;
  margin-top: 72rpx;
}
.content .icon1{
  position: absolute;
  top: 103rpx;
  left: -30rpx;
  width: 143rpx;
  height: 164rpx;
}
.content .icon2{
  position: absolute;
  top: 290rpx;
  right: -30rpx;
  width: 144rpx;
  height: 182rpx;
}
.content .icon3{
  position: absolute;
  bottom: -57rpx;
  right: -30rpx;
  width: 155rpx;
  height: 175rpx;
}
.content .icon4{
  position: absolute;
  bottom: 262rpx;
  left: -30rpx;
  width: 82rpx;
  height: 127rpx;
}
.mask .box{
  width: 600rpx;
  height: 670rpx;
  background-color: #fff;
  border-radius: 20rpx;
  padding: 0 60rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;

}
.mask .title{
  text-align: center;
  color: #3EA7FF;
  font-size: 30rpx;
  width: 450rpx;
  margin-top: 90rpx;
}
.mask .rule{
  width: 476rpx;
  margin-top: 86rpx;
  font-size: 28rpx;
  color: #333333;
}
.titleImage{
  width: 280rpx;
  height: 80rpx;
  margin-top: -40rpx;
}
.btn{
  width: 360rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background: linear-gradient(163deg, #3AA5FF, #2486FF);
  text-align: center;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #fff;
  margin-top: 70rpx;


}