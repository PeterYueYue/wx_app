.page {
  width: 100%;
  min-height: 100vh;
  background: #ffffff;
  box-sizing: border-box;
  padding-top: 40rpx;
  padding-bottom: 228rpx;
}

/* 等级卡片蒙版 */

.lv {
  width: 700rpx;
  height: 320rpx;
  margin: 0 auto;
  background-size: 100% 100%;
  position: relative;
}

.title {
  font-size: 49rpx;
  font-weight: 600;
  color: #FFFFFF;
  line-height: 129rpx;
  text-shadow: 5px 4rpx 16rpx rgba(0, 108, 255, 0.69);
  text-indent: 57rpx;
}

.desc {
  width: 128rpx;
  height: 34rpx;
  line-height: 34rpx;
  text-align: center;
  border: 1rpx solid #FFFFFF;
  border-radius: 17rpx;
  font-size: 23rpx;
  color: #FFFFFF;
  margin-left: 57rpx;
}

.express {
  font-size: 26rpx;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 70rpx;
  margin: 40rpx 0 0 57rpx;
}

.lvpng {
  position: absolute;
  right: 18rpx;
  top: -30rpx;
  width: 178rpx;
}

.progressBox {
  width: 580rpx;
  border-radius: 20rpx;
  margin: 0 0 0 60rpx;
  height: 10rpx;
  line-height: 26rpx;
  font-size: 18rpx;
  position: relative;
  overflow: hidden;
}

.progressBox .line {
  border-radius: 20rpx;
  height: 100%;
  width: 150rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}

.progressBox1 {
  background-color: #0069C9;
}

.progressBox1 .line {
  background: linear-gradient(91deg, #FFFFFF, #A0D3FF);
}

.progressBox2 {
  background-color: #9F531B;
}

.progressBox2 .line {
  background: linear-gradient(91deg, #FFFFFF, #FFD56E);
}

.progressBox3 {
  background-color: #8A430E;
}

.progressBox3 .line {
  background: linear-gradient(91deg, #FFFFFF, #FFD6AE);
}

.progressBox4 {
  background-color: #4F1587;
}

.progressBox4 .line {
  background: linear-gradient(91deg, #FFFFFF, #F5EBFF);
}

.progressBox5 {
  background-color: #006920;
}

.progressBox5 .line {
  background: linear-gradient(91deg, #FFFFFF, #E5FFF5);
}

.progressBox6 {
  background-color: #D83221;
}

.progressBox6 .line {
  background: linear-gradient(91deg, #FFFFFF, #FFDECE);
}

.question {
  width: 700rpx;
  height: 60rpx;
  background: linear-gradient(180deg, #74C4FF, #0081F8);
  border-radius: 30rpx;
  margin: 20rpx auto 0;
  font-size: 24rpx;
  font-weight: 400;
  color: #FFFFFF;
  display: flex;
  align-items: center;
  line-height: 60rpx;
  text-indent: 67rpx;
}
.question .qa {
  width: 160rpx;
  height: 30rpx;
  border-radius: 15rpx;
  background-color: #fff;
  font-size: 22rpx;
  font-weight: 400;
  color: #3AA5FF;
  line-height: 30rpx;
  text-indent: 0;
  text-align: center;
  margin-left: 27rpx;
}
.fa-caret-right {
  margin-left: 10rpx;
}
.now {
  font-size: 38rpx;
  color: #000000;
  line-height: 86rpx;
  text-indent: 47rpx;
}

.flex {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
  margin: 10rpx 20rpx 0 20rpx;
}

.box {
  width: 25%;
  /* width: 124rpx; */
  text-align: center;
  position: relative;
  /* margin: 0 10rpx; */
  box-sizing: border-box;
}

.icon {
  width: 124rpx;
}

.name {
  font-size: 26rpx;
  font-weight: 400;
  color: #333333;
  line-height: 50rpx;
  margin-bottom: 20rpx;
}

.num {
  position: absolute;
  top: 100rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 84rpx;
  height: 30rpx;
  line-height: 30rpx;
  border-radius: 15rpx;
  font-size: 18rpx;
  font-weight: 400;
  color: #FFFFFF;
  padding: 0 10rpx;
  word-spacing: -1px;
}

.num1 {
  background: linear-gradient(144deg, #56AEFF, #7AC8FF);
}

.num2 {
  background: linear-gradient(146deg, #FFCC5E, #FFB422);
}

.num3 {
  background: linear-gradient(146deg, #FFB668, #FF942D);
}

.num4 {
  background: linear-gradient(146deg, #E27BFF, #C054FF);
}

.num5 {
  background: linear-gradient(146deg, #00DBBF, #33E899);
}

.num6 {
  background: linear-gradient(146deg, #FF8155, #FE3E14);
}

/* 报名企业 */

.already {
  width: 450rpx;
  height: 48rpx;
  font-size: 34rpx;
  font-weight: 600;
  color: rgba(0, 108, 255, 1);
  line-height: 60rpx;
  background: url('http://sbag-small.oss-cn-huhehaote.aliyuncs.com/upload/img/web/yy/member/already.png') no-repeat;
  background-size: 100% 100%;
  margin: 71rpx auto 0;
  box-sizing: border-box;
  padding: 0 20rpx;
  text-align: center;
}

.companys {
  width: 750rpx;
  margin-top: 33rpx;
  white-space: nowrap;
  display:flex;
  box-sizing: border-box;
  padding: 0 23rpx;
  flex-direction: row;

}
.companys image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
  flex-shrink:0;
  flex-grow:0;
  box-shadow:1px 0px 4px 2px rgba(43,154,228,0.35);
  
}
.companys .imgbox{
  padding: 15rpx 15rpx;
  display: inline-block;
  vertical-align: top;
  margin-right: 15rpx;
  
  
  
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.activity {
  margin-top: 48rpx;
  margin-bottom: 55rpx;
  width: 430rpx;
}

.small {
  width: 345rpx;
  height: 180rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 2rpx 10rpx 0rpx rgba(0, 90, 213, 0.34);
  border-radius: 20rpx;
  margin: auto;
  display: flex;
  box-sizing: border-box;
  padding: 29rpx 0 29rpx 17rpx;
}

.small image {
  width: 122rpx;
}

.small view {
  flex-direction: row;
}

.small_name {
  padding-left: 25rpx;
}

.small_name view:nth-child(1) {
  font-weight: 600;
  color: rgba(59, 132, 143, 1);
  line-height: 60rpx;
  font-size: 30rpx;
}

.small_name view:nth-child(2) {
  font-weight: 300;
  color: rgba(102, 102, 102, 1);
  line-height: 30rpx;
  font-size: 22rpx;
}

.bottom {
  width: 100%;
  height: 228rpx;
  background: #eee;
  position: fixed;
  bottom: 0;
  left: 0;
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
  line-height: 34rpx;
}

.changeAgree {
  display: flex;
  align-items: flex-start;
  box-sizing: border-box;
  padding: 24rpx 48rpx 0;
}

.agreeicon {
  width: 56rpx;
  margin-right: 12rpx;
}

.btns {
  width: 680rpx;
  height: 100rpx;
  background: #FFFFFF;
  border-radius: 50rpx;
  overflow: hidden;
  margin: 20rpx auto;
  display: flex;
  
}

.btns button {
  width: 50%;
  height: 100rpx;
  border-radius: 0;
  padding: 0;
  height: 100rpx;
  line-height: 100rpx;
  color: #999999;
  font-size: 32rpx;
}
.btns button::after{
  border: none;
}
.btns .buttonP{
  /* background-color: #56AEFF !important; */
  color: #FFFFFF;
  
}


/* 蒙版 */
.level_content {
  width: 600rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
  padding: 50rpx 50rpx 25rpx 40rpx;
}
.level_content .close {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  width: 28rpx;
  height: 30rpx;
}

.level_content .now1 {
  font-size: 34rpx;
  color: rgba(51, 51, 51, 1);
  line-height: 70rpx;
}

.level_content .now text {
  display: inline-block;
  width: 6rpx;
  height: 32rpx;
  background: rgba(58, 165, 255, 1);
  border-radius: 3rpx;
  margin-right: 13rpx;
}

.level_content .level {
  margin-left: 0;
}
.level_content  .level_pic {
  width: 172rpx;
} 
.level_content .progressBox1 {
  max-width: 100%;
  height: 20rpx;
  line-height: 20rpx;
  font-size: 8px;
  background-color: rgba(220, 220, 220, 1);
  text-align: center;
  position: relative;
  margin-top: 20rpx;
  border-radius: 20rpx;
}

.level_content .tips {
  position: relative;
}

.level_content .progressBox1 .line1 {
  background-color: rgba(255, 185, 35, 1);
  border-radius: 20rpx;
  height: 100%;
  width: 150rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}

.level_content .step {
  height: 20rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  display: flex;
  margin-top: 25rpx;
}

.level_content .step view:nth-child(2) {
  text-align: right;
}

.level_content .level_img {
  width: 100%;
  height: 320rpx;
}

.level_content .p1 {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  text-align: center;
  line-height: 40rpx;
}

.level_content .p2 {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  text-decoration: underline;
  color: rgba(62, 167, 255, 1);
  text-align: center;
  line-height: 40rpx;
}

.level_content .p3 {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 70rpx;
  text-indent: 15rpx
}

.level_content .p4 {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 36rpx;
  margin-top: 25rpx;
}

.level_content .p4 text {
  color: #3EA7FF;
}

.table {
  display: flex;
  line-height: 82rpx;
  text-align: center;
  border-radius: 20rpx;
  font-size: 18rpx;
  font-weight: 400;
  line-height: 50rpx;
  color: rgba(58, 165, 255, 1);
  border: 1px solid rgba(58, 165, 255, 1);
  text-align: center;
  overflow: hidden;
}

.table .titles {
  background: rgba(195, 222, 255, 1);
  color: rgba(58, 165, 255, 1);
  font-size: 20rpx;
  height: 82rpx;
  line-height: 82rpx;
}

.table .item {
  width: 33.3%;
}

.table .view3 {
  border-left: 1px solid rgba(58, 165, 255, 1);
  border-right: 1px solid rgba(58, 165, 255, 1);
}
.red {
  color: red;
}