.page {
  width: 100%;
  height: 100vh;
  background: #ffffff;
  box-sizing: border-box;
  padding-bottom: 56rpx;
  display: flex;
  flex-direction: column;
}

.partner {
  width: 100%;
  display: block;
}

.tabs {
  width: 100%;
  height: 94rpx;
  display: flex;
  justify-content: space-around;
  font-size: 32rpx;
  color: #333;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
}

.tabs>view {
  height: 100%;
  box-sizing: border-box;
  line-height: 94rpx;
  position: relative;
  width: 50%;
  text-align: center;
  background-color: rgba(238, 238, 238, 1);
}

.tabs .tab_active {
  font-weight: 700;
  background-color: #fff;
}

.tab_active:before {
  content: '';
  position: absolute;
  left: calc(50% - 50rpx);
  bottom: 0;
  height: 10rpx;
  width: 100rpx;
  background-color: #3aa5ff;
}

swiper-item>scroll-view {
  background-color: #f5f5f5;
  border-radius: 10rpx;
  background-color: #ffffff;
  height: 100%;
  overflow-y: scroll;
}

swiper-item>scroll-view>view {
  /* padding-bottom: 150rpx; */
}

.hint {
  height: calc(100vh - 600rpx);
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  background-color: #ffffff;
}

.list {
  height: 150rpx;
  box-sizing: border-box;
  padding-left: 25rpx;
  padding-right: 25rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
}

.list image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.list .name {
  width: 50%;
  line-height: 50rpx;
}

.list .name view:nth-child(1) {
  font-size: 32rpx;
  color: rgba(51, 51, 51, 1);
}

.list .name view:nth-child(2) {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.list .coin {
  font-size: 28rpx;
  color: rgba(58, 165, 255, 1);
}

.swiper-wrap{
  margin-top: 94rpx;
  box-sizing: border-box;
  flex: 1;
}
.swiper-wrap .swiper{
  height: 100%;
  overflow-y: scroll;
  width: 100%;
}
.hint {
  height: 100vh;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  background-color: #ffffff;
}

.hint>image {
  width: 108rpx;
  margin-bottom: 10rpx;
}