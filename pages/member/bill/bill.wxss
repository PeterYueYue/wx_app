.page {
  width: 100%;
  /* height: 1954rpx;
  min-height: 1421rpx; */
  min-height: calc(100vh - 200rpx);
  box-sizing: border-box;
  background: url('http://image.shishangbag.vip/upload/img/web/yy/member/bill_bg.png') no-repeat;
  background-size: 100% auto;
  overflow: hidden;
  background-color: rgb(72, 215, 235);
}
.height100{
  height: 100vh;
  overflow: hidden;
}
.hh {
  height: 200rpx;
  background-color: rgb(56, 166, 255);
}
.head {
  box-sizing: border-box;
  padding: 0 40rpx 0;
  display: flex;
  align-items: center;
}

.headImg {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 24rpx;
}

.nickname {
  font-size: 42rpx;
  color: rgba(255, 255, 255, 1);
}

.desc {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  margin-top: 18rpx;
}
.level_pic {
  width: 172rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-left: 20rpx;
  
}
.flex {
  color: #fff;
  display: flex;
  justify-content: space-between;
  text-align: center;
  margin: 15rpx 50rpx;
}

.flex .number {
  font-size: 41rpx;
}

.flex .desc {
  font-size: 24rpx;
  position: relative;
}
.flex .desc::before {
  width: 100%;
  height: 12rpx;
  content: "";
  display: block;
  background-color: #2D90FF;
  position: absolute;
  bottom: 0;
  opacity: 0.3;
}
.bill {
  /* height: 1537rpx; */
  /* min-height: 1421rpx; */
  box-sizing: border-box;
  padding-bottom: 10rpx;
  /* background: url('http://image.shishangbag.vip/upload/img/web/yy/member/helpbg.png') no-repeat; */
  background-size: 100% 100%;
  background-color: #fff;
  margin: 26rpx 38rpx 0;
  box-shadow: -1px 4rpx 9rpx 0px rgba(8, 31, 88, 0.16);
  border-radius: 20rpx 20rpx 0 0;
}

.month {
  font-size: 32rpx;
  color: rgba(51, 51, 51, 1);
  line-height: 86rpx;
  text-align: center;
  font-size: 32rpx;
  color: rgba(51, 51, 51, 1);
}
.month image {
  width: 24rpx;
  margin-left: 20rpx;
}

.my {
  box-sizing: border-box;
  padding: 0 35rpx;
}

.my text:nth-child(1) {
  display: inline-block;
  width: 6rpx;
  height: 34rpx;
  background: rgba(58, 165, 255, 1);
  border-radius: 3rpx;
  margin-right: 14rpx;
  vertical-align: top;
}

.my text:nth-child(2) {
  font-size: 34rpx;
  color: rgba(51, 51, 51, 1);
}

.my text:nth-child(3) {
  font-size: 34rpx;
  color: rgba(51, 51, 51, 1);
  float: right;
}

/* ring */

.ring {
  width: 600rpx;
  height: 500rpx;
}

/* 减碳排行 */

.tan {
  text-align: center;
  font-size: 34rpx;
  color: rgba(51, 51, 51, 1);
  display: flex;
  align-items: center;
  justify-content: center;
}

.tan text {
  display: inline-block;
  background-color: blue;
  width: 62rpx;
  height: 20rpx;
  margin: 0 28rpx;
}
.tan text:nth-child(1) {
  background: url('http://image.shishangbag.vip/upload/img/web/yy/member/title_left.png') no-repeat;
  background-size: 100% 100%;
}
.tan text:nth-child(2) {
  background: url('http://image.shishangbag.vip/upload/img/web/yy/member/title_right.png') no-repeat;
  background-size: 100% 100%;
}


.rank_me {
  font-size: 32rpx;
  color: rgba(58, 165, 255, 1);
  line-height: 40rpx;
  text-align: center;
  margin-bottom: 30rpx;
}

/* 排行横向 */

.row {
  display: flex;
  justify-content: space-between;
  margin: 0 41rpx 67rpx;
}
.row .name{
  width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.row>view {
  text-align: center;
  line-height: 45rpx;
}

.row>view:nth-child(1) {
  margin-top: 45rpx;
}

.row>view:nth-child(3) {
  margin-top: 45rpx;
}
.rowhead {
  position: relative;
}
.row .bq {
  width: 146rpx;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}
.row .desc {
  font-size: 18rpx;
  font-weight: 300;
  color: rgba(102, 102, 102, 1);
  margin-top: 0;
}

.rankhead {
  width: 104rpx;
  height: 104rpx;
  background: rgba(238, 238, 238, 1);
  border: 3rpx solid rgba(217, 223, 231, 1);
  border-radius: 50%;
}

.list {
  box-sizing: border-box;
  padding: 0 33rpx;
  margin-top: 34rpx;
  display: flex;
  justify-content: space-between;
}

.list text:nth-of-type(1) {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.list text:nth-of-type(2) {
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.list text:nth-of-type(4) {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  float: right;
}

.btn {
  width: 336rpx;
  height: 46px;
  background: linear-gradient(154deg, rgba(51, 207, 230, 1), rgba(58, 165, 255, 1));
  border-radius: 47rpx;
  border: none;
  margin: 37rpx auto 0;
  color: #fff;
}

/* 无数据情况 */

.norank {
  margin: 26rpx auto 0;
  text-align: center;
}
.norank image {
  width: 200rpx;
}

.bill .p1 {
  font-size: 28rpx;
  color: rgba(51, 51, 51, 1);
  text-align: center;
  margin-top: 35rpx;
}

.bill .p2 {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  text-align: center;
  margin-top: 17rpx;
}
.bill_bottom {
  height: 136rpx;
  width: 699rpx;
  margin: -1rpx 0 24rpx 38rpx;
  background: url('http://image.shishangbag.vip/upload/img/web/yy/member/bill_bottom.png') no-repeat;
  background-size: 100% 100%;
}

.f2-chart {
  width: 100%;
  height: 500rpx;
}




/* canvas */

.model-wrap {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 99999;
  width: 750rpx;
  height: 100vh;
  overflow: hidden;
}
.model-box {
	position: absolute;
	top: 50%;
	left: 0;
	transform: translate(0, -50%);
}
.model-wrap image {
  position: absolute;
	top: 290rpx;
	left: 75rpx;
  width: 600rpx;
  height: 740rpx;
  z-index: 99;
  
  
}
.canvas2 {
	position: absolute;
	top: 260rpx;
	left: 75rpx;
	width: 600rpx;
	height: 740rpx;
}
.save-btn {
	text-align: center;
	color: #fff;
	position: absolute;
	top: 90%;
	left: 43%;
	transform: translate(0, -50%);
}

.model-wrap .btns{
  display: flex;
  position: absolute;
  width: 100%;
  bottom: 100rpx;
  left: 0;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.model-wrap .btn{
  width: 280rpx;
  height: 46px;
  line-height: 46px;
  font-size: 16px;
  text-align: center;
}
.zindex .model-wrap{
  
  z-index: 99999999;
}
.point{
  width: 3px;
  height: 3px;
  border-radius: 50%;
  background-color: #3AA5FF;
  margin-right: 8rpx;
}
.listTop{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.lookbill {
  font-size: 28rpx;
  color: #FFFFFF;
  background: linear-gradient(181deg, #FFD173, #FFAC1C);
  border-radius: 45rpx;
  display: flex;
  width: 336rpx;
  height: 90rpx;
  font-size: 28rpx;
  align-items: center;
  justify-content: center;
}