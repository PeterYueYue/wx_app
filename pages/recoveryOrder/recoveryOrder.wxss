page {
  background-color: #f5f5f5;
}

.page {
  width: 100%;
  /* height: 100vh; */
  background-color: #f5f5f5;
  box-sizing: border-box;
  padding: 20rpx 20rpx 51rpx;
}

.orderFlag {
  text-align: center;
  margin-top: 19rpx;
  margin-bottom: 22rpx;
}

.flagImg {
  width: 127rpx;
  margin-bottom: 3rpx;
}

.flagText {
  font-size: 28rpx;
  color: #666;
  font-weight: 550;
}

.order_box {
  width: 710rpx;
  /* height: 214rpx; */
  background-color: #fff;
  box-shadow: 2rpx 3rpx 2rpx 2rpx rgba(58, 58, 58, 0.16);
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 34rpx 28rpx 27rpx;
  margin-bottom: 35rpx;
}

.order_title {
  display: flex;
  justify-content: space-between;
  font-size: 34rpx;
  font-weight: 700;
  margin-bottom: 28rpx;
}

.departmentName {
  font-size: 28rpx;
  font-weight: 600;
  color: #666;
  /* margin-top: 29rpx; */
}

.order_num {
  color: #1a1a1a;
}

.order_flag {
  color: #3aa5ff;
}

.order_text {
  margin-top: 28rpx;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  color: #333;
  font-weight: 600;
}

.steps {
  position: relative;
  height: 90rpx;
}

.round {
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  background-color: #3aa5ff;
  border-radius: 50%;
  margin-left: 28rpx;
}

.r_text {
  font-size: 30rpx;
  color: #333;
  font-weight: 600;
  margin-left: 24rpx;
}

.r_time {
  font-size: 30rpx;
  color: #333;
  margin-left: 52rpx;
  font-weight: 600;
}

.line {
  width: 3rpx;
  height: 80rpx;
  background-color: #3aa5ff;
  position: absolute;
  top: 20rpx;
  left: 37rpx;
}

.steps_box .steps:last-child .line {
  display: none;
}

.weight_box {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.weight {
  font-size: 30rpx;
  font-weight: 600;
  color: #333;
}

.del {
  width: 140rpx;
  height: 50rpx;
  border-radius: 10rpx;
  border: 1rpx solid #3aa5ff;
  text-align: center;
  line-height: 50rpx;
}

.evaluation {
  width: 100%;
  height: 650rpx;
  background-color: #ffffff;
  box-shadow: 2rpx 3rpx 2rpx 2rpx rgba(58, 58, 58, 0.16);
  border-radius: 10rpx;
  margin-top: 24rpx;
  padding: 24rpx 36rpx 0;
  box-sizing: border-box;
}

.driver_title {
  display: flex;
  align-items: center;
}

.driver_t {
  width: 90rpx;
  border-radius: 50%;
}

.driver_name {
  color: #333;
  font-size: 32rpx;
  font-weight: 600;
  width: 360rpx;
  margin-left: 27rpx;
}

.radio_box {
  display: flex;
  align-items: center;
}

.radio {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  background-color: #3aa5ff;
  border-radius: 50%;
  border: 1rpx solid #3aa5ff;
  text-align: center;
  color: #fff;
  font-size: 20rpx;
  line-height: 30rpx;
}

.radio1 {
  width: 28rpx;
}

.radio_text {
  display: inline-block;
  font-size: 28rpx;
  color: #3aa5ff;
  margin-left: 11rpx;
}

.radio_f {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  background-color: #b3b3b3;
  border-radius: 50%;
  border: 1rpx solid #ccc;
  text-align: center;
  color: #fff;
  font-size: 20rpx;
  line-height: 30rpx;
}

.radio_text_f {
  display: inline-block;
  font-size: 28rpx;
  color: #b3b3b3;
  margin-left: 11rpx;
}

.evaluationBox {
  padding: 30rpx 50rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-around;
}

.evaluation_box {
  text-align: center;
}

.evaluation_img {
  width: 65rpx;
}

.evaluation_text {
  font-size: 28rpx;
  color: #999;
  margin-top: 22rpx;
}

.evaluation_text_t1 {
  font-size: 28rpx;
  color: #69b5f5;
  margin-top: 22rpx;
}

.evaluation_text_t2 {
  font-size: 28rpx;
  color: #ffaf31;
  margin-top: 22rpx;
}

.evaluation_text_t3 {
  font-size: 28rpx;
  color: #f16c1b;
  margin-top: 22rpx;
}

textarea {
  width: 100%;
  height: 220rpx;
  border: 1rpx solid #bfbfbf;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 20rpx 27rpx;
  margin-top: 5rpx;
}

.placeholder {
  font-size: 26rpx;
  color: #999999;
  letter-spacing: 1px;
}

.submit {
  width: 230rpx;
  height: 85rpx;
  border-radius: 43rpx;
  border: solid 1rpx #3ea7ff;
  text-align: center;
  line-height: 85rpx;
  margin: 23rpx auto 0;
  font-size: 36rpx;
  color: #58b1ff;
}

.expect {
  width: 687rpx;
  height: 70rpx;
  background-image: linear-gradient(175deg, rgba(255, 175, 4, 0.88) 0%, rgba(248, 116, 2, 0.88) 46%, rgba(240, 56, 0, 0.88) 100%), linear-gradient( #ffffff, #ffffff);
  background-blend-mode: normal, normal;
  margin: -20rpx auto 0;
  border-radius: 35rpx;
}

.gold {
  width: 33rpx;
  height: 32rpx;
  margin-left: 28rpx;
  animation: bounce-down 1.8s linear infinite;
}

.prompt {
  display: flex;
  /* justify-content: center; */
  align-items: center;
  height: 100%;
}

.table {
  width: 100%;
  /* height: 1464rpx; */
  background-color: #ffffff;
  box-shadow: 2rpx 3rpx 2rpx 2rpx rgba(58, 58, 58, 0.16);
  border-radius: 10rpx;
  margin-top: 33rpx;
}

.table_title {
  box-sizing: border-box;
  padding: 34rpx 30rpx 0 30rpx;
}

.table_num {
  font-size: 34rpx;
  color: #1a1a1a;
  font-weight: 700;
  margin-bottom: 34rpx;
}

.delivery {
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  color: #333;
  margin-bottom: 32rpx;
  font-weight: 600;
}
.delivery text {
  font-size: 26rpx;
  color: #666;
}
.table_line {
  height: 2rpx;
  width: 637rpx;
  background-color: #e5e5e5;
  margin: auto;
}

.table_content {
  box-sizing: border-box;
  padding: 28rpx 38rpx 15rpx 30rpx;
}

.table_row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 28rpx;
}

.table_row_img {
  width: 62rpx;
}

.table_row_type {
  font-size: 30rpx;
  color: #666;
  font-weight: 700;
  width: 180rpx;
  margin-left: 22rpx;
}

.table_row_referencePrice {
  font-size: 28rpx;
  color: #666;
  width: 220rpx;
  /* text-align: right; */
  display: flex;
  justify-content: flex-end;
}

.table_row_price {
  color: #333333;
  font-size: 30rpx;
  width: calc(100% - 474rpx);
  text-align: right;
}

.wrong_delivery {
  box-sizing: border-box;
  padding: 53rpx 30rpx 48rpx 30rpx;
}

.wrong_delivery_title {
  font-size: 34rpx;
  font-weight: 700;
  color: #1a1a1a;
  margin-bottom: 25rpx;
}

.wrong_delivery_img {
  width: 200rpx;
  height: 200rpx;
  margin-right: 25rpx;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}

.wrong_delivery_img:nth-child(3n) {
  margin-right: 0;
}

.wrong_delivery_describe {
  font-size: 30rpx;
  color: #333;
  font-weight: 700;
  margin-top: 9rpx;
  display: flex;
}
.time {
  font-size: 26rpx;
  color: #666;
  margin-bottom: 30rpx;
}
.text_1 {
  width: 200rpx;
}

.text_2 {
  width: calc(100% - 200rpx);
}
/* 添加用券 */
.smq2 text {
  padding: 0 20rpx;
  height:40rpx;
  background:rgba(255,255,255,1);
  border:1px solid #FF9626;
  border-radius:10px;
  text-align: center;
  color: #FF9626;
  line-height: 40rpx;
  font-size: 22rpx;
  margin: 0 20rpx;
  display: inline-block;
  margin-bottom: 10rpx;
}
@keyframes bounce-down {
  20% {
    transform: translateY(-75rpx);
  }
  35% {
    transform: translateY(-80rpx);
  }
  40% {
    transform: translateY(0)
  }
  45% {
    transform: translateY(-28rpx);
  }
  65% {
    transform: translateY(-45rpx);
  }
  70% {
    transform: translateY(0);
  }
  75% {
    transform: translateY(-15rpx);
  }
  80% {
    transform: translateY(0)
  }
  100% {
    transform: translateY(0)
  }
}