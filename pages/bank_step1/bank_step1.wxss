/* uploader */

.ui_uploader_cell {

}

.ui_uploader_item {
  float: left;
  position: relative;
  width: 165rpx;
  height: 165rpx;
}

.ui_uploader {
  float: left;
  position: relative;
  width: 165rpx;
  height: 165rpx;
  border: 2rpx solid #d9d9d9;
  box-sizing: border-box;
}

.ui_uploader:before {
  content: " ";
  position: absolute;
  width: 4rpx;
  height: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.ui_uploader:after {
  content: " ";
  position: absolute;
  height: 4rpx;
  width: 79rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.ui_uploader_item_icon {
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  background: #fff;
  border-radius: 50%;
}

.ui_icon {
  display: block;
}

.ui_uploader_item image {
  width: 100%;
  height: 100%;
}
.card_picture{
  border-bottom: 1rpx solid #ededed;
  font-weight: 600;
  color: #1A1A1A;
  font-size: 28rpx;
  height: 250rpx;
  box-sizing: border-box;
  padding: 0 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}