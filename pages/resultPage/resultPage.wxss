.page{
  width: 100%;
  display: flex;
  flex-direction: column;
  flex: 1;
  background-color: #FFFFFF;
  align-items: center;
  padding-top: 63rpx;
  
}
.resultImg{
  width: 250rpx;
  height: 250rpx;
}
.title{
  font-size: 40rpx;
  color: #0B89FF;
  margin-top: 30rpx;
}
.subTitleBox{
  color: #118DFF;
  font-size: 28rpx;
  margin-top: 50rpx;
}
.subTitleBox .subtitle {
  margin: 0 10rpx;
}
.content{
  width: 650rpx;
  color: #333333;
  margin-top: 30rpx;
}
.content .titel{
  font-size: 22rpx;

}
.content .list{
  margin-top: 30rpx;
  font-size: 22rpx;
  color: #656565;
  line-height: 40rpx;
}
.thanks{
  width: 457rpx;
  height: 152rpx;
  margin-top: 60rpx;
}
.btnBox{
  display: flex;
  margin-top: 30rpx;
  
}
.btnBox .btn{
  width: 250rpx;
  height: 46px;
  border-radius:10rpx;
  color: #FFFFFF;
  font-size: 16px;
  line-height: 46px;
  text-align: center;

}
.btnBox .detail{
  background-color: #B7EB7F;
  margin-right: 67rpx;
}
.btnBox .back{
  background-color: #39A5FF;
}