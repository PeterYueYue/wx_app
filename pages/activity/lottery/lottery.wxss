.page {
  width: 100%;
  min-height: 100vh;
  background-repeat: no-repeat;
  overflow: hidden;
  background-size:cover;
}
.content {
  width: 80%;
  min-height: 400rpx;
  box-sizing: border-box;
  padding: 40rpx;
  background-color: #fff;
  font-size: 28rpx;
  border-radius: 10rpx;
  line-height: 50rpx;
  color: 999;
}
.btns {
  width: 100%;
  margin: 119rpx 0 0;
  display: flex;
  justify-content: space-between;
}

.btns view {
  width: 170rpx;
  height: 60rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  text-align: center;
}
.btns view:nth-child(1) {
  color: #C54700;
  background-color: #FFDC9E;
  border-top-right-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
}
.btns view:nth-child(2) {
  background-color: rgba(5, 88, 75, 0.76);
  color: #fff;
  border-top-left-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
}
.wheelBg {
  width: 100%;
  height: 1000rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin: 10rpx auto;
  text-align: center;
  overflow: hidden;
  position: relative;
}
.wheel {
  width: 528rpx;
  height: 528rpx;
  margin: 80rpx auto 0;
  transform: scaleX(1) scaleY(1);
  background-size: 100%;
}
.button {
  width: 300rpx;
  margin: 80rpx auto 0;
}
.go {
  width: 170rpx;
  height: 170rpx;
  position: absolute;
  top: 270rpx;
  left: 50%;
  transform: translateX(-50%);
}
.choose {
  width: 84rpx;
  position: absolute;
  top: 0rpx;
  left: 50%;
  transform: translateX(-50%);
}
.residue {
  text-align: center;
  color: #fff;
  margin-top: -120rpx;
  z-index: 1000;
  position: relative;
  z-index: 99;
}
.residue text {
  color: #FF8D30;
}