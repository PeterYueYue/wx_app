/* 蒙版 */
.success_content {
  width: 530rpx;
  height: 520rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx;
  text-align: center;
  padding-top: 85rpx;
  box-sizing: border-box;
}

.success_content .head {
  font-size: 38rpx;
  font-weight: 500;
  color: rgba(255, 105, 21, 1);
  line-height: 96rpx;
  margin-top: 20rpx;
}

.success_content image {
  width: 188rpx;
  height: 188rpx;
}

.success_content .p1 {
  font-size: 34rpx;
  color: #333;
  margin-top: 64rpx;
}

.success_content .p2 {
  font-size: 30rpx;
  color: #333333;
  margin-top: 41rpx;
}
/* 登录按钮 */
.success_content button {
  width:440rpx;
  height:88rpx;
  line-height: 88rpx;
  background:rgba(58,165,255,1);
  border-radius:44rpx;
  color: #fff;
  margin: 20rpx auto 0;
  font-size: 36rpx;
}
