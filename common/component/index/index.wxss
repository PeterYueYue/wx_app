.index_page {
  width: 100%;
  min-height: 100vh;
  position: relative;
  background-color: #fff;
}

.index_page .top {
  width: 100%;
  min-height: 500rpx;
  position: relative;
  line-height: 0px;
}

.index_page .imgTop {
  width: 100%;
  z-index: -1;
}

.index_page .img {
  width: 88rpx;
  position: absolute;
  animation: updown 2s infinite;
}

.index_page .img1 {
  top: 146rpx;
  left: 40rpx;
  animation-delay: 0.2s;
}

.index_page .img2 {
  width: 86rpx;
  top: 72rpx;
  left: 230rpx;
  animation-delay: 0.4s;
}

.index_page .img3 {
  width: 48rpx;
  top: 125rpx;
  left: 527rpx;
  animation-delay: 0.6s;
}

.index_page .img4 {
  top: 180rpx;
  right: 24rpx;
  animation-delay: 0.8s;
}

@keyframes updown {
  50% {
    transform: translateY(20rpx);
  }
  100% {
    transform: translateY(0rpx);
  }
}

.index_page .msg {
  width: 720rpx;
  height: 150rpx;
  background: linear-gradient(185deg, #30A0FD, #27AFFF);
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  position: absolute;
  bottom: 14rpx;
  left: 15rpx;
  display: flex;
  align-items: center;
}

.avatarbox {
  width: 154rpx;
  text-align: center;
  margin: 0 20rpx 0 29rpx;
}

.avatarbox .avatar{
  width: 105rpx;
  height: 105rpx;
}
.msg .avatar {
  width: 105rpx;
  border-radius: 50%;
}

.msg .level_pic {
  width: 154rpx;
  margin-top: -10rpx;
}

.msg .infobox {
  line-height: 40rpx;
  margin: 0 14rpx 0 0;
}

.msg .uninfobox {
  text-indent: 60rpx;
  line-height: 50rpx;
}

.msg .name {
  font-size: 28rpx;
  font-weight: 600;
  color: #FFFFFF;
}

.msg .experience {
  font-size: 24rpx;
  font-weight: 400;
  color: #FFFFFF;
  opacity: 0.77;
}

.msg .progressBox {
  width: 272rpx;
  border-radius: 20rpx;
  height: 10rpx;
  line-height: 26rpx;
  font-size: 18rpx;
  position: relative;
  overflow: hidden;
  background-color: #0069C9;
  margin-top: 10rpx;
}

.msg .progressBox .line {
  height: 100%;
  width: 150rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
  background: linear-gradient(91deg, #FFFFFF, #A0D3FF);
  border-radius: 4rpx;
}

.msg .couponbox {
  width: 200rpx;
  height: 66rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  line-height: 66rpx;
  background-size: 100% 100%;
  text-indent: 35rpx;
}

.index_page .topbottom {
  width: 100%;
  height: 14rpx;
  background-color: #fff;
  position: absolute;
  bottom: 0rpx;
  left: 0;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  z-index: 2;
}

/* 导航区域 */

.index_page .nav-wrap {
  background-color: #fff;
  overflow: hidden;
  box-sizing: border-box;
  padding: 0 50rpx;
}

.index_page .nav {
  width: 100%;
  display: flex;
  box-sizing: border-box;
  justify-content: space-between;
}

.index_page .item {
  flex-wrap: nowrap;
  flex: 1;
  text-align: center;
}

.index_page .item view {
  font-size: 30rpx;
  color: #333333;
}

.index_page .nav .nav-img {
  width: 165rpx;
}

/* 轮播图 */

.index_page .swiper-wrap {
  overflow: hidden;
  padding-bottom: 20rpx;
  border-radius: 10rpx;
  margin-top: 10rpx;
  background-color: #fff;
}

.index_page swiper {
  width: 686rpx;
  margin: 20rpx auto 0;
  border-radius: 10rpx;
  height: 300rpx;
  overflow: hidden;
  transform: translateY(0);
}

.index_page .swiper-item {
  display: block;
  height: 300rpx;
  width: 100%;
  border-radius: 10rpx;
  overflow: hidden;
}

.index_page .swiper-item image {
  width: 100%;
  height: 300rpx;
  border-radius: 10rpx;
}

/* 拾尚互动 */

.index_page .interaction {
  margin: 10rpx auto 0;
  width: 690rpx;
  height: 310rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 3rpx 7rpx 1rpx rgba(0, 0, 0, 0.15);
  border-radius: 20rpx;
  overflow: hidden;
}

.interaction .head {
  display: flex;
  align-items: center;
  margin: 20rpx 0;
}

.interaction .head text {
  display: inline-block;
  width: 6rpx;
  height: 26rpx;
  background: #3AA5FF;
  border-radius: 3rpx;
  margin: 0 17rpx 0 28rpx;
}

.interaction .flex {
  display: flex;
  align-items: center;
  justify-content: space-around;
  box-sizing: border-box;
  padding: 0 23rpx;
}

.flex .item {
  width: 200rpx;
  height: 200rpx;
  position: relative;
}
.flex .item:nth-child(2) {
  margin: 0 20rpx;
}

.flex .item image {
  width: 100%;
}

.index_page .rule {
  display: flex;
  justify-content: space-between;
  width: 690rpx;
  margin: 30rpx auto 0;
}

.index_page .rule image {
  width: 160rpx;
  margin: 0 0 0 19rpx;
}
.index_page .interaction .begin {
  width: 100%;
  position: absolute;
  top: 73rpx;
  left:0rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: #fff;
  z-index: 10;
  padding-left: 25rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.index_page .interaction .timeitem {
  display: inline-block;
  vertical-align: top;
  width: 26rpx;
  height: 26rpx;
  border-radius: 2px;
  background-color: #fff;
  box-sizing: border-box;
  color: #FF9A00;
  font-size: 16rpx;
  line-height: 26rpx;
  margin: 0 2rpx;
}
.index_page .interaction .people {
  font-size: 23rpx;
  font-weight: 400;
  position: absolute;
  top: 73rpx;
  left: 25rpx;
  color: #fff;
  line-height: 30rpx;
  text-align: left;
}
.index_page .interaction .people text {
  color: #FF6C00;
}
