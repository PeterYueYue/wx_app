  border-bottom-left-radius: 20px;
  position: absolute;
  color: #fff;
  right: 0rpx;
  top: 60rpx;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  text-indent: 15rpx;
}


/* 本次收益 */
.self_page .expect {
