.self_page {
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
  padding-bottom: 30rpx;
}

.self_page .self_head {
  height: 390rpx;
   background: url('http://image.shishangbag.vip/upload/img/web/yy/member/normalbg.png') no-repeat;
   background-size: 100% 100%;
}
.self_page .self_head_plus {
  height: 390rpx;
  background: url('http://image.shishangbag.vip/upload/img/web/yy/member/plusbg.png') no-repeat;
  background-size: 100% 100%;
}

.self_page .info {
  width: 690rpx;
  background: rgba(255, 255, 255, 1);
