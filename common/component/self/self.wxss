}
.self_page .headImg_plus {
  background: url('http://image.shishangbag.vip/upload/img/web/yy/member/plushead.png') no-repeat;
  background-size: 100% 100%;
} 

.self_page .headImg .head {
  width: 152rpx;
  height: 152rpx;
  border-radius: 50%;
  margin: 4rpx 8rpx 0 0;
}
.self_page .plus_icon{
  width: 74rpx;
  position: absolute;
  bottom: 0;
  left: 50rpx;
}

.self_page .nickname {
  color: #333;
  font-size: 34rpx;
